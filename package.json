{
	"name": "flowcatalyst-typescript",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "pnpm --filter @flowcatalyst/flowcatalyst build",
		"build:full": "pnpm --filter @flowcatalyst/flowcatalyst build:full",
		"build:sea": "pnpm --filter @flowcatalyst/flowcatalyst build:sea",
		"build:frontend": "pnpm --filter @flowcatalyst/platform-frontend build",
		"dev": "pnpm --filter @flowcatalyst/flowcatalyst dev",
		"dev:frontend": "pnpm --filter @flowcatalyst/platform-frontend dev",
		"test": "pnpm --filter @flowcatalyst/flowcatalyst test",
		"typecheck": "pnpm --filter @flowcatalyst/flowcatalyst typecheck",
		"lint": "oxlint .",
		"lint:fix": "oxlint --fix .",
		"format": "oxfmt --write .",
		"format:check": "oxfmt --check .",
		"clean": "rm -rf apps/flowcatalyst/dist apps/platform-frontend/dist node_modules",
		"db:generate": "pnpm --filter @flowcatalyst/flowcatalyst db:generate",
		"db:migrate": "pnpm --filter @flowcatalyst/flowcatalyst db:migrate",
		"db:push": "pnpm --filter @flowcatalyst/flowcatalyst db:push",
		"db:studio": "pnpm --filter @flowcatalyst/flowcatalyst db:studio"
	},
	"devDependencies": {
		"@types/node": "^24.0.0",
		"oxfmt": "^0.28.0",
		"oxlint": "^1.43.0",
		"typescript": "^5.7.3"
	},
	"engines": {
		"node": ">=24.0.0"
	},
	"packageManager": "pnpm@9.15.4",
	"pnpm": {
		"overrides": {
			"drizzle-orm": "1.0.0-beta.15-859cf75"
		},
		"packageExtensions": {
			"drizzle-kit": {
				"dependencies": {
					"drizzle-orm": "*"
				}
			}
		}
	}
}
