{
  "name": "flowcatalyst-typescript",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@9.15.4",
  "engines": {
    "node": ">=24.0.0"
  },
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm -r --parallel dev",
    "test": "pnpm -r test",
    "test:watch": "pnpm -r --parallel test:watch",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "typecheck": "pnpm -r typecheck",
    "clean": "pnpm -r clean && rm -rf node_modules"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.10.7",
    "typescript": "^5.7.3",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "neverthrow": "^8.2.0"
  },
  "pnpm": {
    "overrides": {
      "drizzle-orm": "1.0.0-beta.15-859cf75"
    },
    "packageExtensions": {
      "drizzle-kit": {
        "dependencies": {
          "drizzle-orm": "*"
        }
      }
    }
  }
}
