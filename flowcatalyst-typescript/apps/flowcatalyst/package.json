{
	"name": "@flowcatalyst/flowcatalyst",
	"version": "0.0.1",
	"description": "FlowCatalyst - Unified app with Platform, Message Router, and Stream Processor",
	"type": "module",
	"scripts": {
		"dev": "tsx watch src/index.ts",
		"build": "tsup && [ -d drizzle ] && rm -rf ./dist/drizzle && cp -r drizzle ./dist/drizzle || true && rm -rf ./dist/public && cp -r public ./dist/public",
		"build:full": "pnpm --filter @flowcatalyst/platform-frontend build && pnpm run build && cp -r ../platform-frontend/dist ./dist/frontend",
		"build:sea": "pnpm --filter @flowcatalyst/platform-frontend build && tsup && node scripts/pack-migrations.js && node scripts/pack-frontend.js && node --experimental-sea-config sea-config.json && node scripts/build-sea.js",
		"start": "node dist/index.js",
		"typecheck": "tsc --noEmit",
		"test": "vitest run",
		"test:watch": "vitest",
		"openapi:extract": "tsx scripts/extract-openapi.ts",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "drizzle-kit migrate",
		"db:push": "drizzle-kit push",
		"db:studio": "drizzle-kit studio"
	},
	"dependencies": {
		"@aws-sdk/client-elastic-load-balancing-v2": "^3.717.0",
		"@aws-sdk/client-sqs": "^3.717.0",
		"@fastify/cookie": "^11.0.2",
		"@fastify/cors": "^10.0.2",
		"@fastify/static": "^9.0.0",
		"@fastify/swagger": "^9.4.2",
		"@fastify/swagger-ui": "^5.2.1",
		"@fastify/type-provider-typebox": "^5.1.0",
		"@sinclair/typebox": "^0.34.30",
		"argon2": "^0.41.1",
		"cockatiel": "^3.2.1",
		"dotenv": "^16.4.7",
		"drizzle-orm": "1.0.0-beta.15-859cf75",
		"fastify": "^5.2.1",
		"fastify-plugin": "^5.0.1",
		"ioredis": "^5.9.3",
		"jose": "^6.1.3",
		"nats": "^2.29.3",
		"neverthrow": "^8.2.0",
		"nodemailer": "^7.0.12",
		"oidc-provider": "^9.1.0",
		"openid-client": "^6.8.1",
		"pino": "^9.6.0",
		"postgres": "^3.4.5",
		"prom-client": "^15.1.3",
		"rate-limiter-flexible": "^9.0.1",
		"sql.js": "^1.12.0",
		"stompit": "^1.0.0",
		"undici": "^7.3.0",
		"zod": "^4.1.13"
	},
	"devDependencies": {
		"@types/node": "^22.10.7",
		"@types/nodemailer": "^7.0.5",
		"@types/oidc-provider": "^9.5.0",
		"@types/sql.js": "^1.4.9",
		"@types/stompit": "^0.26.6",
		"drizzle-kit": "1.0.0-beta.15-859cf75",
		"pino-pretty": "^13.1.3",
		"postject": "^1.0.0-alpha.6",
		"tsup": "^8.5.1",
		"tsx": "^4.19.2",
		"typescript": "^5.9.3",
		"vite-tsconfig-paths": "^5.1.4",
		"vitest": "^4.0.18"
	},
	"optionalDependencies": {
		"@aws-sdk/client-secrets-manager": "^3.700.0",
		"@aws-sdk/client-ssm": "^3.700.0"
	}
}
